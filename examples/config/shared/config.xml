<?xml version="1.0"?>
<Partition xmlns="http://www.fentiss.com/xngPartitionXml" name="Config" entryPoint="0x12000000" attributes="system boot">
  <MemoryAreas>
    <Area start="0x12000000" size="64MB" type="nonSharedNormal" access="readwrite executable" cachePolicy="l1WriteBackWa l2WriteBackWa" />
    <Area start="0x16000000" size="16MB" type="nonSharedNormal" access="readonly" cachePolicy="l1WriteBackWa l2WriteBackWa" />
    <!-- GPIO --> <Area start="0x80000000" size="32KB" type="shareableDevice" access="readwrite" cachePolicy="none" />
  </MemoryAreas>
  <HmTable baseHmTable="defaultHmTab" exceptionToEvent = "prefetchExcp dataExcp" nbHmLogs="64" />
  <Devices>
  </Devices>
  <Ports>
    <SamplingPort name="RouterConfig" direction="source" irq="166"/>
  </Ports>
</Partition>
